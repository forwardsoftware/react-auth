{
  "name": "@forward-software/react-auth",
  "version": "1.1.0",
  "author": "ForWarD Software (https://github.com/Forward-Software)",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "description": "Simplify your Auth flow when working with React apps",
  "repository": "https://github.com/Forward-Software/react-auth",
  "homepage": "https://github.com/Forward-Software/react-auth#readme",
  "keywords": [
    "react",
    "react-native",
    "auth",
    "authentication"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-auth.esm.js",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src test --ext .ts,.tsx",
    "lint:fix": "eslint src test --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "examples:base": "yarn --cwd examples/base",
    "examples:reqres": "yarn --cwd examples/reqres",
    "examples:refresh-token": "yarn --cwd examples/refresh-token",
    "examples:native": "yarn --cwd examples/native"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/use-sync-external-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8"
  }
